<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Res Config Settings -->
    <!-- Res Config Settings Inherit Form View-->
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.realstate</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="45"/>
        <field name="inherit_id" ref="real_estate.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='property_partner_id']" position="after">
                <label for="sale_commission"/>
                <field name="sale_commission" />
            </xpath>
        </field>
    </record>
    

</odoo>
