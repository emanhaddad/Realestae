<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for Real estate maintenance-->
        <record id="seq_real_estate_maintenance" model="ir.sequence">
            <field name="name">Real Estate Maintenance</field>
            <field name="code">real.estate.maintenance</field>
            <field name="prefix">REM/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="real_estate_booking_time" model="ir.sequence">
            <field name="name">Booking</field>
            <field name="code">real.estate.booking</field>
            <field name="prefix">RB/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_real_estate_sale" model="ir.sequence">
            <field name="name">Real Estate Sale</field>
            <field name="code">real.estate.sale</field>
            <field name="prefix">RES/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_real_estate_deliver" model="ir.sequence">
            <field name="name">Real Estate delivery</field>
            <field name="code">real.estate.delivery</field>
            <field name="prefix">RED/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_real_estate_delegate" model="ir.sequence">
            <field name="name">Real Estate delegate</field>
            <field name="code">real.estate.delegate</field>
            <field name="prefix">RDE/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_maintenance_request_seq" model="ir.sequence">
            <field name="name">maintenance request sequence</field>
            <field name="code">real.estate.maintenance</field>
            <field eval="5" name="padding"/>
             <field name="prefix">JOB</field>
            <field eval="False" name="company_id"/>
        </record>
    </data>

</odoo>
