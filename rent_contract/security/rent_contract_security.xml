<odoo>
	<data noupdate="1">
     <!-- category rent contract -->

           <record id="rent_contract_access_categ" model="ir.module.category">
           		<field name="name">Contract Management</field>
           </record> 

           <!-- group of contract view-->
           <record id="contract_group" model="res.groups">
               <field name="name">Contracts</field>
               <field name="comment">Contracts access Group</field>
               <field name="category_id" ref="rent_contract_access_categ"/>
           </record> 

            <!-- group of payment list view-->
           <record id="payment_list_group" model="res.groups">
               <field name="name">Payments List</field>
               <field name="comment">Payments List Group</field>
               <field name="category_id" ref="rent_contract_access_categ"/>
               <field name="implied_ids" eval="[(4, ref('rent_contract.contract_group'))]"/>
           </record> 

            <!-- group of payment list view-->
           <record id="realestate_contract_group" model="res.groups">
               <field name="name">Realestate Contract </field>
               <field name="comment">Realestate Contract Group</field>
               <field name="category_id" ref="rent_contract_access_categ"/>
               <field name="implied_ids" eval="[(4, ref('rent_contract.contract_group'))]"/>
           </record>

           

           <!-- group of payment list view-->
           <record id="re_expired_contract_group" model="res.groups">
               <field name="name">Expired Contract Contract </field>
               <field name="comment">Contract  Group</field>
               <field name="category_id" ref="rent_contract_access_categ"/>
               <field name="implied_ids" eval="[(4, ref('rent_contract.contract_group'))]"/>
           </record>
           

            <!-- group of contract configuration-->
           <record id="contract_configuration_group" model="res.groups">
               <field name="name">Configuration</field>
               <field name="comment">Configuration access Group</field>
               <field name="category_id" ref="rent_contract_access_categ"/>
           </record> 


           <!-- group of valid contract  button-->
           <record id="valid_contracts_group" model="res.groups">
               <field name="name">Valid Contract Button</field>
               <field name="comment">Valid Contract Button access Group</field>
               <field name="category_id" ref="rent_contract_access_categ"/>
           </record>

           <record id="end_contracts_group" model="res.groups">
               <field name="name">End Contract Button</field>
               <field name="comment">End Contract Button access Group</field>
               <field name="category_id" ref="rent_contract_access_categ"/>
           </record>  

           <!-- group of expired contract  button-->
           <!-- Not used until now-->
           <record id="expired_contracts_group" model="res.groups">
               <field name="name">Expired Contract Button</field>
               <field name="comment">Expired Contract Button access Group</field>
               <field name="category_id" ref="rent_contract_access_categ"/>
           </record> 

           <!-- group of create payments  button-->
           <!-- Not used untill now -->
           <record id="create_payments_group" model="res.groups">
               <field name="name">Create Payments Button</field>
               <field name="comment">Create Payments Button access Group</field>
               <field name="category_id" ref="rent_contract_access_categ"/>
           </record> 

           <!-- group of generate payments reciept button-->
           <record id="generate_payment_reciept_group" model="res.groups">
               <field name="name">Generate Payment Reciept Button</field>
               <field name="comment">Generate Payment Reciept access Group</field>
               <field name="category_id" ref="rent_contract_access_categ"/>
           </record> 

           <!-- group of generate payments reciept button-->
           <record id="rent_with_min_group" model="res.groups">
               <field name="name">Rent With Min</field>
               <field name="comment">rent realestate with min than thire monthly cost</field>
               <field name="category_id" ref="rent_contract_access_categ"/>
           </record> 

           <!-- group of generate payments reciept button-->
           <record id="rent_with_previous_date" model="res.groups">
               <field name="name">Rent With Previous Date</field>
               <field name="comment">rent realestate with previous Date</field>
               <field name="category_id" ref="rent_contract_access_categ"/>
           </record> 

         
           <record id="return_supervisor_group" model="res.groups">
               <field name="name"> Contracts supervisor</field>
               <field name="comment">supervisor access Group</field>
               <field name="category_id" ref="rent_contract_access_categ"/>
               <field name="implied_ids" eval="[(4, ref('rent_contract.expired_contracts_group'))]"/>
           </record> 




    </data>
</odoo>