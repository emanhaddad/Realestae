<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_over_time" model="ir.sequence">
            <field name="name">Overtime</field>
            <field name="code">hr.overtime</field>
            <field name="prefix">OV/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_payroll_over_time" model="ir.sequence">
            <field name="name">Payroll Overtime</field>
            <field name="code">hr.payroll.overtime</field>
            <field name="prefix">POV/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </date>
</odoo>