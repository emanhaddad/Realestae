<?xml version="1.0" ?>
<odoo>
    <data noupdate='1'>
		<record id="ir_seq_app_script_hr_loan" model="ir.sequence">
			<field name="name">Loan Request</field>
			<field name="code">hr.loan.seq</field>
			<field name="prefix">LO/</field>
			<field name="padding">4</field>
			<field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
		</record>
		<record id="ir_seq_app_script_hr_loan_payment" model="ir.sequence">
			<field name="name">Loan Payment</field>
			<field name="code">hr.loan.payment</field>
			<field name="prefix">LP/</field>
			<field name="padding">4</field>
			<field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
		</record>
		<record id="ir_seq_app_script_hr_loan_suspend" model="ir.sequence">
			<field name="name">Loan Suspend</field>
			<field name="code">hr.loan.suspend</field>
			<field name="prefix">LS/</field>
			<field name="padding">4</field>
			<field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
		</record>
	</data>
</odoo>
